[extension_name]
Get Folder Files
[extension_info]
Scan a folder and get an array of all files inside of it
[insert_external]
<!--
 * @name: Get Folder Files, @version: 1.0, @author: Christina K., @source: https://christinna9031.itch.io/
 * -->
<div><p>Get Folder Files is installed</p></div>
[insert_command]
LB.extCommand('Get Folder Files', 3355443, 52, {
  value: ['Value', 15, '', 0.5 ],
  path: ['Path', 15, '', 0.5 ],
  filters: ['Filter Types', 14, '', 2 ],
  sort: ['Sort Order', 19, 'alphabetical', null, ['alphabetical', 'numerical' ]],
  save: ['Save As', 19, 'File Name', null, ['File Name', 'Full Path' ]],
})
[insert_hook]
case "Get Folder Files":
LBGetFolderFiles(LioranBoardJSON.value, LioranBoardJSON.path, LioranBoardJSON.filters, LioranBoardJSON.sort, LioranBoardJSON.save, LioranBoardJSON.FromButton)  
break;
[insert_script]
function LBGetFolderFiles(val, path, filterTypes, sort, save, btn) {
    // create array of all file names
    let array = val.split('\r\n');
    for (let i = 0; i < array.length; i++) {
        if (array[i] === '') {
            array.splice(i, 1);
        }
    }
    // check if folder exists (will automatically default to Receiver folder if not)
    if ((array.includes('LioranBoard 2.0.exe') && !path.includes('lioranboard'))) {
        NoFilesFound();
        return;
    }
    // remove unwanted file types
    if (filterTypes.length > 1) {
        array = array.filter((file) => filterTypes.indexOf(file.split('.')[1]) !== -1 || filterTypes.indexOf(file.split('.')[2]) !== -1);
    }
    // sort either numerically or alphabetically
    array.sort();
    if (sort === 'numerical') {
        array.sort((a, b) => a.split('.')[0] - b.split('.')[0]);
    }
    // append path if full path is required
    if (save === 'Full Path') {
        for (let i = 0; i < array.length; i++) {
            array[i] = (`${path.trim()}/${array[i]}`).replace('//', '/');
        }
    }
    // check if array is empty and notify LB
    if (array.length === 0) {
        NoFilesFound();
        return;
    }
    LB.setVariable('files', array, btn);

    function NoFilesFound() {
        LB.setVariable('files', [], btn);
        LB.alert('Get Folder Files extension: No files found. Check your path and filters.');
    }
}
